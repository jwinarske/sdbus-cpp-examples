            <arg type="a{sv}" name="options" direction="in"/>
        </method>
        <method name="Stop">
            <arg type="a{sv}" name="options" direction="in"/>
        </method>
        <method name="SetLabel">
            <arg type="s" name="label" direction="in"/>
            <arg type="a{sv}" name="options"
