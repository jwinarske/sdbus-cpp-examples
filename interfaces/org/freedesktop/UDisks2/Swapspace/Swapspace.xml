        </method>
        <method name="SetUUID">>
            <arg type="s" name="uuid" direction="in"/>
            <arg type="a{sv}" name="options"
                 direction="in"/>
        </method>
        <property type="b" name="Active" access="read"/>
    </interface>
</node>